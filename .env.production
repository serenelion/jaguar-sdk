# Jaguar Monorepo - Production Environment Variables
# Copy this file to .env for production deployment

# ===== AI Open Agents Configuration =====

# OpenRouter Credentials (Required)
OPEN_ROUTER_API_BASE=https://openrouter.ai/api/v1
OPEN_ROUTER_API_KEY=your-open-router-api-key-here

# Tool Server API Key
TOOL_SERVER_API_KEY=your-secure-tool-server-api-key-here

# Supabase Configuration (Required)
SUPABASE_API_URL=https://your-project.supabase.co
SUPABASE_API_KEY=your_supabase_api_key_here

# ===== GitHub Integration =====

# GitHub Personal Access Token (Required for GitHub integration)
GITHUB_TOKEN=ghp_your_github_personal_access_token_here

# GitHub Webhook Secret (Optional, for webhook validation)
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# ===== Jaguar SDK Configuration =====

# Database (Required for Jaguar SDK)
POSTGRES_URL=postgresql://postgres:password@your-db-host:5432/postgres

# Jaguar AI Models - Production (connects to ai.thespatialnetwork.net)
JAGUAR_BASE_URL=https://ai.thespatialnetwork.net
JAGUAR_API_KEY=your-openwebui-api-key-here

# Authentication for Jaguar SDK (Required)
AUTH_SECRET=your-auth-secret-here-minimum-32-characters-long-production
NEXTAUTH_SECRET=your-nextauth-secret-here-minimum-32-characters-long
NEXTAUTH_URL=https://jaguar-sdk.thespatialnetwork.net

# Optional Enhancements
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_your-production-token
REDIS_URL=redis://redis:6379

# ===== Production Configuration =====

# Node Environment
NODE_ENV=production

# ===== Self-Development Configuration =====

# Default repository for self-development tasks
DEFAULT_REPO_OWNER=your-github-username
DEFAULT_REPO_NAME=jaguar-monorepo

# AI Agent Configuration
ENABLE_SELF_DEVELOPMENT=true
SELF_DEV_INTERVAL_HOURS=24
MAX_CONCURRENT_TASKS=3

# ===== Docker Registry Configuration =====

# GitHub Container Registry (for image publishing)
GHCR_TOKEN=ghp_your_github_token_with_packages_write_permission
GHCR_USERNAME=your-github-username

# ===== Monitoring & Logging =====

# Optional: Application monitoring
SENTRY_DSN=your-sentry-dsn-here
LOG_LEVEL=info

# ===== Security =====

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=https://jaguar-sdk.thespatialnetwork.net,https://ai.thespatialnetwork.net

# Rate limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# ===== Feature Flags =====

# Enable/disable features in production
ENABLE_ANALYTICS=true
ENABLE_ERROR_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true
